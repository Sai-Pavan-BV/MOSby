#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55fdb77759d0 .scope module, "pc_tb" "pc_tb" 2 2;
 .timescale 0 0;
v0x55fdb7789bc0_0 .net "address", 15 0, v0x55fdb773acf0_0;  1 drivers
v0x55fdb7789ca0_0 .var "bra_add", 15 0;
v0x55fdb7789d70_0 .var "branch", 0 0;
v0x55fdb7789e70_0 .var "clk", 0 0;
v0x55fdb7789f40_0 .var "increment", 0 0;
v0x55fdb778a030_0 .var "rst", 0 0;
S_0x55fdb7775b60 .scope module, "p" "pc" 2 6, 3 1 0, S_0x55fdb77759d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "branch";
    .port_info 3 /INPUT 1 "increment";
    .port_info 4 /INPUT 16 "bra_add";
    .port_info 5 /OUTPUT 16 "address";
v0x55fdb773acf0_0 .var "address", 15 0;
v0x55fdb77896f0_0 .net "bra_add", 15 0, v0x55fdb7789ca0_0;  1 drivers
v0x55fdb77897d0_0 .net "branch", 0 0, v0x55fdb7789d70_0;  1 drivers
v0x55fdb7789870_0 .net "clk", 0 0, v0x55fdb7789e70_0;  1 drivers
v0x55fdb7789930_0 .net "increment", 0 0, v0x55fdb7789f40_0;  1 drivers
v0x55fdb7789a40_0 .net "rst", 0 0, v0x55fdb778a030_0;  1 drivers
E_0x55fdb773b630 .event posedge, v0x55fdb7789870_0;
E_0x55fdb775e9b0 .event edge, v0x55fdb7789a40_0;
    .scope S_0x55fdb7775b60;
T_0 ;
    %wait E_0x55fdb775e9b0;
    %load/vec4 v0x55fdb7789a40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 65482, 0, 16;
    %cassign/vec4 v0x55fdb773acf0_0;
    %jmp T_0.1;
T_0.0 ;
    %deassign v0x55fdb773acf0_0, 0, 16;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55fdb7775b60;
T_1 ;
    %wait E_0x55fdb773b630;
    %load/vec4 v0x55fdb77897d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55fdb77896f0_0;
    %store/vec4 v0x55fdb773acf0_0, 0, 16;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55fdb7789930_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x55fdb773acf0_0;
    %addi 1, 0, 16;
    %store/vec4 v0x55fdb773acf0_0, 0, 16;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x55fdb773acf0_0;
    %store/vec4 v0x55fdb773acf0_0, 0, 16;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55fdb77759d0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55fdb7789e70_0;
    %inv;
    %store/vec4 v0x55fdb7789e70_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55fdb77759d0;
T_3 ;
    %vpi_call 2 9 "$dumpfile", "tb.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000001, S_0x55fdb7775b60 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fdb7789e70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fdb7789f40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fdb778a030_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fdb778a030_0, 0, 1;
    %delay 40, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fdb7789d70_0, 0, 1;
    %vpi_func 2 16 "$random" 32 {0 0 0};
    %pad/s 16;
    %store/vec4 v0x55fdb7789ca0_0, 0, 16;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fdb7789d70_0, 0, 1;
    %delay 40, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fdb7789f40_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "pc_tb.v";
    "./pc.v";
